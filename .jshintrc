{
  "bitwise": false,
  "curly": true,
  "eqeqeq": true,
  "es3": false,
  "forin": true,
  "freeze": true,
  "latedef": "nofunc",
  "noarg": true,
  "nonbsp": true,
  "nonew": true,
  "plusplus": false,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "strict": false,
  "maxparams": 10,
  "maxdepth": 5,
  "asi": false,
  "boss": true,
  "debug": false,
  "eqnull": true,
  "esnext": true,
  "evil": false,
  "expr": true,
  "funcscope": false,
  "iterator": false,
  "lastsemic": false,
  "loopfunc": true,
  "maxerr": 20,
  // If false will break IDE when lot errors
  "moz": false,
  "notypeof": true,
  "proto": false,
  "scripturl": false,
  "shadow": false,
  "sub": true,
  "supernew": false,
  "validthis": false,
  "noyield": false,
  "browser": true,
  "devel": true,
  "node": true,
  "laxbreak": true,
  "jasmine": true,
  "globalstrict": false,
  "maxstatements": 40,
  "maxcomplexity": 8,
  "predef": [
    "-Promise"
  ],
  "globals": {
    "_": true,
    "$": false
  }
}
